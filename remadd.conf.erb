{
	"ssh-key-storage": "/var/ssh-key-storage",
	"binaries": { 
		"KadminMit": "/usr/bin/kadmin",
		"KadminHeimdal": "/usr/bin/kadmin.heimdal"
	},
	"loglevel": "debug",


	"realm": {
		"realm1": { "keytab": "/file1", "principal": "princ@realm1", "type": "KadminImpl" },
		"RSYSLOG3": { "keytab": "/etc/krb5.keytab", "principal": "host/<%= @fqdn %>@RSYSLOG3", "type": "<% case @kdc_impl %><% when "mit" %>KadminMit<% when "heimdal" %>KadminHeimdal<% end %>" }
	},


	"groups": {
		"groupname1": ["member1", "member2"],
		"group_selftest": ["host/<%= @fqdn %>@RSYSLOG3"]
	},


	"acls": {
		"createkeytab": [
			{ "group": "groupname2", "host": "regex", "services": "regex" },
			{ "group": "group_selftest", "host": "^ztook[0-9]?.meta.zcu.cz$", "services": "^(host|ftp|nfs|pbs)$" }
		],

		"storesshhostkey": [
			{ "group": "group_selftest", "host": "^ztook[0-9]?.meta.zcu.cz$", "filename": "test_remad_.*" }
		],

		"getsshhostkey": [
			{ "group": "group_selftest", "host": "^ztook[0-9]?.meta.zcu.cz$", "filename": "test_remad_.*" }
		]

	}
}
